<h2 class="mt-3">Vista de Productos</h2>
<div class="container-sm mt-5">

        <table class="table table-dark table-striped">
          <thead>
            <tr>
              <th scope="col">Titulo</th>
              <th scope="col">Precio</th>
              <th scope="col">Imagen</th>
            </tr>
          </thead>
          <tbody>
           
            {{#each productos}}
            <tr>
              <td>{{title}}</td>
              <td>${{price}}</td>
              <td><img src="{{thumbnail}}" width="50px" height="50px"></td>
            </tr>
            {{/each}} 

          </tbody>

        </table>

</div>
 <div class="container">
    <h2 class="text-center mt-3">Vista de Productos</h2>
    
    <div class="d-grid gap-2 d-md-flex justify-content-md-end"></div>
      <a href="/" class="btn btn-primary">Cargar Producto</a>
    </div>

      <div class="container-sm mt-5">

        <table class="table table-dark table-striped">
      <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Titulo</th>
        <th scope="col">Precio</th>
        <th scope="col">Imagen</th>
    </tr>
  </thead>
  <tbody>
    {{#if listExist}}
    {{#each dataProductos}}
    <tr>
        <td>{{id}}</td>
        <td>{{title}}</td>
        <td>${{price}}</td>
        <td><img src="{{thumbnail}}" width="50px" height="50px"></td>
    </tr>
    {{/each}} 
    {{/if}}
  </tbody>

  </table>

      </div>

   </div>  
  
  
  